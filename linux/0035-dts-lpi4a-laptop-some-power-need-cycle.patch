From 9208f2e68765518ef059154f7ea8b3f4b551513a Mon Sep 17 00:00:00 2001
From: Lu Hui <luhui@sipeed.com>
Date: Mon, 20 Nov 2023 15:51:17 +0800
Subject: [PATCH 35/53] dts: lpi4a-laptop: some power need cycle

---
 arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts | 3 ---
 1 file changed, 3 deletions(-)

diff --git a/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts b/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts
index 5a2aeaf1e..e361462f9 100644
--- a/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts
+++ b/arch/riscv/boot/dts/thead/light-lpi4a-laptop.dts
@@ -60,7 +60,6 @@ reg_usb_vcc_5v: regulator-usb-vcc-5v-en {
 		vin-supply = <&reg_sys_vcc_5v>;
 		gpio = <&pcal6408ahk_d 3 1>;
 		enable-active-high;
-		regulator-boot-on;
 		regulator-always-on;
         };
 
@@ -73,7 +72,6 @@ reg_sata_vcc_5v: regulator-sata-vcc-5v-en {
 		vin-supply = <&reg_sys_vcc_5v>;
 		gpio = <&pcal6408ahk_c 1 1>;
 		enable-active-high;
-		regulator-boot-on;
 		regulator-always-on;
 	};
 
@@ -84,7 +82,6 @@ audio_amp: regulator-audio-amp-en {
                 vin-supply = <&reg_sys_vcc_5v>;
 		gpio = <&pcal6408ahk_c 3 1>;
 		enable-active-high;
-		regulator-boot-on;
 		regulator-always-on;
 	};
 
